{
  "extends": "airbnb",
  "rules": {
    "react/jsx-one-expression-per-line": [0],
    "react/jsx-filename-extension": [0],
    "no-param-reassign": [0],
    "max-len": [0],
    "react/jsx-tag-spacing": [1, {
      beforeSelfClosing: "allow"
    }],
  },
  "env": {
    "browser": true,
    "node": true,
  },
}
